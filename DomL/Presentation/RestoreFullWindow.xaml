<Window x:Class="DomL.Presentation.RestoreFullWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DomL.Presentation"
        mc:Ignorable="d"
        Title="RestoreFullWindow" Height="450" Width="800">
    <DockPanel>
        <Button Height="30" Width="100" Margin="10" Content="Book" Click="BookButton_Click"/>
        <Button Height="30" Width="100" Margin="10" Content="Comic" Click="ComicButton_Click"/>
        <Button Height="30" Width="100" Margin="10" Content="Game" Click="GameButton_Click"/>
        <Button Height="30" Width="100" Margin="10" Content="Series" Click="SeriesButton_Click"/>
        <Button Height="30" Width="100" Margin="10" Content="Watch" Click="WatchButton_Click"/>
    </DockPanel>
</Window>
